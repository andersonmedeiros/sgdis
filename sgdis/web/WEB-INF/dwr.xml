    <convert converter="bean" match="model.bean.Turma"/>
    <convert converter="bean" match="model.dao.TurmaDAO"/>
    <convert converter="bean" match="model.bean.Tentativa"/>
    <convert converter="bean" match="model.dao.TentativaDAO"/>
    <convert converter="bean" match="model.bean.Aluno"/>
    <convert converter="bean" match="model.dao.AlunoDAO"/>
  </allow>
</dwr>
