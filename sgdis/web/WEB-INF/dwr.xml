    <convert converter="bean" match="model.bean.Estado"/>
    <convert converter="bean" match="model.dao.EstadoDAO"/>    
    <convert converter="bean" match="model.bean.Cidade"/>
    <convert converter="bean" match="model.dao.CidadeDAO"/>    
    <convert converter="bean" match="model.bean.ForcaHasEstado"/>
    <convert converter="bean" match="model.dao.ForcaHasEstadoDAO"/>    
